{
  "name": "myTest",
  "displayName": "myTest",
  "description": "this is my test extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "testing.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "myTest.helloTest",
        "title": "Create Test"
      }
    ],
    "keybindings": [
        {
          "command": "myTest.helloTest",
          "key": "ctrl+f1",
          "mac": "cmd+f1",
          "when": "editorTextFocus"
        }
      ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  }
}
